{

  "Logging": {

    "LogLevel": {

      "Default": "Information",

      "Microsoft.AspNetCore": "Warning"

    }

  },

  "ConnectionStrings": {

    "DefaultConnection": "${DB_CONNECTION_STRING}"

  },

  "Jwt": {

    "Key": "${JWT_KEY}", // https://jwtsecrets.com = generate a new one (256 bits)

    "RefreshTokenKey": "${JWT_REFRESH_KEY}", // https://jwtsecrets.com = generate a new one (512 bits)

    "Issuer": "App",

    "Audience": "UserApp"

  },

  "AllowedHosts": "*"

}

